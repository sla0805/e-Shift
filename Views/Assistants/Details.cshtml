@model eShift.Models.Assistant

@{
    ViewData["Title"] = "Assistant Info";
}

<h2>Assistant Details</h2>

<div>
    <h4>Assistant</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Assistant ID</dt>
        <dd class="col-sm-10">@Model.AssistantId</dd>

        <dt class="col-sm-2">Assistant Name</dt>
        <dd class="col-sm-10">@Model.AssistantName</dd>

        <dt class="col-sm-2">Phone</dt>
        <dd class="col-sm-10">@Model.AssistantPhone</dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.AssistantId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>